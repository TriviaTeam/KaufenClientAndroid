<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MarketSessionActivity">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/section_search"
                android:layout_width="0dp"
                android:layout_height="187dp"
                android:background="@android:color/background_light"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="152dp"
                android:layout_height="82dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/section_search"
                app:layout_constraintEnd_toEndOf="@+id/section_search"
                app:layout_constraintStart_toStartOf="@+id/section_search"
                app:layout_constraintTop_toTopOf="@+id/section_search"
                app:layout_constraintVertical_bias="0.137"
                app:srcCompat="@drawable/logocru" />

            <EditText
                android:id="@+id/pesquisar_mercado"
                android:layout_width="175dp"
                android:layout_height="38dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:ems="10"
                android:hint="Perquisar mercado"
                android:inputType="textPersonName"
                android:textColorLink="@color/primary_material_light_1"
                android:textCursorDrawable="@color/primary_material_light_1"
                app:layout_constraintBottom_toBottomOf="@+id/section_search"
                app:layout_constraintEnd_toEndOf="@+id/section_search"
                app:layout_constraintHorizontal_bias="0.378"
                app:layout_constraintStart_toStartOf="@+id/section_search"
                app:layout_constraintTop_toBottomOf="@+id/imageView8"
                app:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:id="@+id/button_search_maerket"
                android:layout_width="40dp"
                android:layout_height="34dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="36dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="36dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toBottomOf="@+id/section_search"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.058"
                app:layout_constraintStart_toEndOf="@+id/pesquisar_mercado"
                app:layout_constraintTop_toBottomOf="@+id/imageView8"
                app:layout_constraintVertical_bias="0.885"
                app:srcCompat="@drawable/searchicon" />

            <ImageView
                android:id="@+id/extra_section"
                android:layout_width="0dp"
                android:layout_height="235dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/mercado"
                android:contentDescription="TODO"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/section_search" />

            <ImageView
                android:id="@+id/extra_section_logo"
                android:layout_width="82dp"
                android:layout_height="82dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/extra_section"
                app:layout_constraintEnd_toEndOf="@+id/extra_section"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/extra_section"
                app:layout_constraintVertical_bias="0.262"
                app:srcCompat="@drawable/extra_icon" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Extra"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="45sp"
                app:layout_constraintBottom_toBottomOf="@+id/extra_section"
                app:layout_constraintEnd_toEndOf="@+id/extra_section"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="@+id/extra_section"
                app:layout_constraintTop_toBottomOf="@+id/extra_section_logo"
                app:layout_constraintVertical_bias="0.187" />

            <ImageView
                android:id="@+id/carrefour_section"
                android:layout_width="0dp"
                android:layout_height="255dp"
                android:layout_marginTop="4dp"
                android:background="@drawable/mercado"
                android:contentDescription="TODO"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/extra_section" />

            <ImageView
                android:id="@+id/carrefour_section_logo"
                android:layout_width="82dp"
                android:layout_height="82dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="56dp"
                app:layout_constraintEnd_toEndOf="@+id/carrefour_section"
                app:layout_constraintStart_toStartOf="@+id/carrefour_section"
                app:layout_constraintTop_toTopOf="@+id/carrefour_section"
                app:srcCompat="@drawable/carrefour_icon" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Carrefour"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="45sp"
                app:layout_constraintBottom_toTopOf="@+id/pao_de_acucar_section"
                app:layout_constraintEnd_toEndOf="@+id/carrefour_section"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="@+id/carrefour_section"
                app:layout_constraintTop_toBottomOf="@+id/carrefour_section_logo"
                app:layout_constraintVertical_bias="0.363" />

            <ImageView
                android:id="@+id/pao_de_acucar_section"
                android:layout_width="0dp"
                android:layout_height="255dp"
                android:layout_marginTop="4dp"
                android:background="@drawable/mercado"
                android:contentDescription="TODO"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/carrefour_section" />

            <ImageView
                android:id="@+id/pao_de_acucar_section_logo"
                android:layout_width="82dp"
                android:layout_height="82dp"
                android:layout_marginBottom="136dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/carrefour_section"
                app:layout_constraintStart_toStartOf="@+id/carrefour_section"
                app:srcCompat="@drawable/paodeacucar_icon" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Pão de Açucar"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="45sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/carrefour_section"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="@+id/carrefour_section"
                app:layout_constraintTop_toBottomOf="@+id/pao_de_acucar_section_logo"
                app:layout_constraintVertical_bias="0.169" />

        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>